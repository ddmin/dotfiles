	"keyMappings": [
		"#!no-check",
		"\" video speed controller",
		"\tunmap [[",
		"\tunmap ]]",
		"",
		"\" remove lowercase bindings",
		"\tunmap d",
		"\tunmap f",
		"\tunmap i",
		"\tunmap p",
		"\tunmap r",
		"\tunmap u",
		"\tunmap v",
		"",
		"\" lowercase: new tab, UPPERCASE: current tab",
		"\tmap O Vomnibar.activate",
		"\tmap o Vomnibar.activateInNewTab",
		"",
		"\tmap B Vomnibar.activateBookmarks",
		"\tmap b Vomnibar.activateBookmarksInNewTab",
		"",
		"\" replace lowercase with UPPERCASE",
		"\tmap F LinkHints.activate",
		"\tmap I enterInsertMode",
		"\tmap V LinkHints.activateSelect",
		"",
		"\" tab navigation",
		"\tmap L nextTab",
		"\tmap H previousTab",
		"",
		"\" history navigation",
		"\tmap <c-r> goForward",
		"\tmap <c-z> goBack",
		"\tmap u goBack",
		"",
		"\" page zoom",
		"\tmap K zoomIn",
		"\tmap J zoomOut",
		"\tmap zi zoomIn",
		"\tmap zo zoomOut",
		"\tmap zz zoomReset",
		"",
		"\" create a bookmark",
		"\"   firefox: \"Bookmarks Toolbar\"",
		"\"   chrome:  \"Bookmarks\"",
		"\tmap <c-b> addBookmark folder=\"Bookmarks\"",
		"",
		"\" delete a tab",
		"\tmap ZZ removeTab",
		"\tmap x removeTab",
		"\tmap X restoreTab",
		"",
		"\" control key bindings",
		"\tmap <c-d> scrollPageDown",
		"\tmap <c-e> scrollDown",
		"\tmap <c-u> scrollPageUp",
		"\tmap <c-y> scrollUp",
		"\tmap <c-g> enterFindMode",
		"\tmap <c-v> LinkHints.activateHover",
		"",
		"\" reposition tabs",
		"\tmap <c-s-h> moveTabLeft",
		"\tmap <c-s-l> moveTabRight",
		"",
		"\" y: yank",
		"\tmap yi LinkHints.activateDownloadImage",
		"\tmap yI LinkHints.activateCopyImage",
		"",
		"\" t: tab bindings (and yank alias)",
		"\tmap gt Vomnibar.activateTabs",
		"\tmap gT nextTab",
		"\tmap tt copyCurrentUrl",
		"\tmap TT copyCurrentTitle",
		"\tmap tf LinkHints.activateCopyLinkUrl",
		"",
		"\" D: duplicate",
		"\tmap DD duplicateTab",
		"\tmap DF LinkHints.activateWithQueue",
		"",
